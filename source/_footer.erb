    <div id="Footer"  class="container">
        <div class="row">
            <div class="col-sm-4 col-md-4 author">
                <h2 class="category">かいてるひと</h2>
                <img src="http://ppcdn.500px.org/53778294/0c55021febf0fbc7f529cb96d27cfd7f67f5d779/3.jpg" alt="ハノイ大聖堂 by Akihiro Nagashima on 500px.com" border="0" style="margin: 0 0 5px 0;" width="220px">
                <p>
                    <i class="icon-twitter-sign"></i>
                    <a href="https://twitter.com/eiei19" target="_blank"> @eiei19</a>
                    <br />
                    <i class="icon-facebook-sign"></i>
                    <a href="https://www.facebook.com/nagashima.akihiro" target="_blank"> nagashima.akihiro</a>
                    <br />
                    <i class="icon-camera"></i>
                    <a href="http://500px.com/eiei19" target="_blank">500px</a>
                </p>
            </div>
            <div class="col-sm-4 col-md-4">
                <h2 class="category">あたらしいやつ</h2>
                <ul>
                    <% blog.articles[0...5].each do |article| %>
                    <li><%= link_to article.title, article %> <span><%= article.date.strftime(' (%m月%d日)') %></span></li>
                    <% end %>
                </ul>
                <h2 class="category">ぶんべつしたやつ</h2>
                <ul>
                    <% blog.tags.each do |tag, articles| %>
                    <li><%= link_to tag, tag_path(tag) %> (<%= articles.size %>)</a></li>
                    <% end %>
                </ul>
                <h2 class="category">つきごと</h2>
                <ul>
                    <% blog.articles.reverse.group_by {|article| article.date.year.to_s()+'/'+article.date.month.to_s() }.each do |year_month, articles| %>
                    <% year = year_month.split("/")[0] %>
                    <% month = year_month.split("/")[1] %>
                    <li><%= link_to year+"年"+month+"月", blog_month_path(year, month) %> (<%= articles.size %>)</a></li>
                    <% end %>
                </ul>
            </div>
            <div class="col-sm-4 col-md-4">
                <h2 class="category">よんだほん</h2>
                <script type="text/javascript" src="http://widget.booklog.jp/blogparts/js/booklog_minishelf.js?wood_disp" id="booklog_minishelf"></script>
                <script type="text/javascript" src="http://api.booklog.jp/json/eiei19?category=0&count=15&callback=booklog_minishelf"></script>
            </div>
        </div>
